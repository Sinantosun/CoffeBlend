@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/UILayout/Index.cshtml";
}

@await Component.InvokeAsync("_DefaultAboutComponentPartial")
@await Component.InvokeAsync("_DefaultServicesComponentPartial")
@await Component.InvokeAsync("_DefaultMenuComponentPartial")
@await Component.InvokeAsync("_DefaultStatisticComponentPartial")
@await Component.InvokeAsync("_DefaultCoffeProductsComponentPartial")
@await Component.InvokeAsync("_DefaultGaleryComponentPartial")
@await Component.InvokeAsync("_DefaultTestimonialComponentPartial")
@await Component.InvokeAsync("_DefaultProductsComponentPartial")
@await Component.InvokeAsync("_DefaultReservationComponentPartial")

@if (TempData["reservationstatus"] != null)
{
    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>

    <script>
        Swal.fire({
            title: "Rezervasyon",
            text: "Rezervasyonunuz alındı en kısa sürede size dönüş sağlanacaktır.",
            icon: "success"
        });
    </script>
}

